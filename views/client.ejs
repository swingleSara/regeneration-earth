<%- include('partials/header') -%>
<div class="container">
  <div class="row mt-1">
    <!--Left half-->
    <div id="left-col" class="h-100 col-6 fixed-top bkg">
      <div class="mt-3 mb-3 fnt-wht">
        <form action="/file/getClient" enctype="application/x-www-form-urlencoded" method="GET">
          <div class="mb-3">
            <label for="code" class="form-label">Code</label>
            <input type="text" class="form-control bkg-light" id="code" name="code">
          </div>
          <div class="mb-3">
            <label for="word" class="form-label">Word</label>
            <input type="text" class="form-control bkg-light" id="word" name="word">
          </div>
          <div class="mb-3">
          <button type="submit" class="btn btn-lg form-control" value="Submit">Submit</button>
        </form>
      </div>
    </div>
    <!--Right half-->
    <div class="offset-sm-6">
      <div class="container">
        <div><a href="<%=pair.link %>">Google Folder Link</a></div>
      </div>
    </div>  
  </div>
</div>
<%- include('partials/footer') -%>